package advent

import (
	"fmt"
	"testing"

	"github.com/stretchr/testify/assert"
)

func DayNN(input string) (int, int) {
	tot := 0
	for _, l := range Lines(input) {
		fmt.Println(l)
	}
	return tot, 0
}

func TestDayNN_Ex1(t *testing.T) {
	a, b := DayNN(exDNN)
	assert.Equal(t, a, 42)
	assert.Equal(t, b, 42)
}

func TestDayNN_Actual(t *testing.T) {
	a, b := DayNN(inputDNN)
	fmt.Printf("Day NN Part 1: %d\n", a)
	fmt.Printf("Day NN Part 2: %d\n", b)
}

const exDNN = ``

const inputDNN = ``
